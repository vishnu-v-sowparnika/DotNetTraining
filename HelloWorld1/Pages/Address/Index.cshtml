@page
@model HelloWorld1.Pages.Address.IndexModel
@{

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>

    <h3>Address List</h3>
    <table border="1" cellpadding="2" cellspacing="2">
        <tr>

            <th>Name</th>
            <th>PO -Box</th>


        </tr>
        @foreach (var add in Model.address)
        {
            <tr>
                <td>@add.Name</td>
                <td>@add.Pobox</td>


            </tr>
        }
    </table>

    <div class="text-center">
        <h1 class="display-4">Add Address</h1>
    </div>

    <form method="post" asp-page="Address">
        <div class="form-group">
            <label name="Name"></label>
            <input name="Name" class="form-control" placeholder="Enter Name" autocomplete="off" />
        </div>
        <div class="form-group">
            <label name="Name"></label>
            <input name="Name" class="form-control" placeholder="Enter Name" autocomplete="off" />
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>

    </form>

</body>
</html>
